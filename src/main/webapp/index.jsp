<header class="page-header" id="Ocompany">
    <h1>About Our Company</h1>
    <p class="header-subtitle">Your trusted partner in real estate, providing exceptional service and expertise to help you achieve your property goals.</p>
</header>


<section class="about-section">
    <div class="about-container">
        <div class="about-image">
            <img src="https://images.unsplash.com/photo-1560518883-ce09059eeffa?ixlib=rb-4.0.3" alt="Real Estate Property">
        </div>
        <div class="about-content">
            <h2>Excellence in Real Estate Management</h2>
            <p class="description">With years of experience in the real estate industry, we pride ourselves on delivering exceptional property management services that exceed our clients' expectations.</p>
            <p class="sub-description">Our dedicated team of professionals is committed to providing comprehensive solutions for property buyers, sellers, and renters, ensuring a seamless experience throughout every transaction.</p>

            <div class="stats-container">
                <div class="stat-item">
                    <h2>500+</h2>
                    <p>Properties Managed</p>
                </div>
                <div class="stat-item">
                    <h2>1000+</h2>
                    <p>Happy Clients</p>
                </div>
                <div class="stat-item">
                    <h2>15+</h2>
                    <p>Years Experience</p>
                </div>
            </div>
        </div>
    </div>
    <header class="page-header">
        <h1>Contact Us</h1>
        <p class="header-subtitle">Get in touch with our expert team for all your real estate needs</p>
    </header>

    <section class="contact-section">
        <div class="contact-container" id="Cpage">
            <div class="contact-info">
                <h2>Get In Touch</h2>
                <div class="info-item">
                    <h3>Address</h3>
                    <p>123 Real Estate Avenue<br>Business District, NY 10001</p>
                </div>
                <div class="info-item">
                    <h3>Phone</h3>
                    <p>+1 (555) 123-4567</p>
                </div>
                <div class="info-item">
                    <h3>Email</h3>
                    <p>info@realestate.com</p>
                </div>
                <div class="info-item">
                    <h3>Business Hours</h3>
                    <p>Monday - Friday: 9:00 AM - 6:00 PM<br>Saturday: 10:00 AM - 4:00 PM</p>
                </div>
            </div>

            <form class="contact-form" method="post" action="feedback">
                <div class="form-group">
                    <label for="name">Full Name</label>
                    <input type="text" id="name" name="name" required>
                </div>
                <div class="form-group">
                    <label for="email">Email</label>
                    <input type="email" id="email" name="email" required>
                </div>
                <div class="form-group">
                    <label for="phone">Phone</label>
                    <input type="tel" id="phone" name="phone">
                </div>
                <div class="form-group">
                    <label for="subject">Subject</label>
                    <select id="subject" name="subject">
                        <option value="property-management">Property Management</option>
                        <option value="buying">Buying Property</option>
                        <option value="selling">Selling Property</option>
                        <option value="other">Other</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="message">Message</label>
                    <textarea id="message" name="message" rows="5" required></textarea>
                </div>
                <button type="submit" class="submit-btn">Send Message</button>
                <%
                    if(request.getAttribute("success")!=null) {
                %>
                <div id="myModal" class="modal">
                    <div class="modal-content">
                        <span class="close" onclick="closeModal()">&times;</span>
                        <p style="color: #4cae4c"><%=request.getAttribute("success")%></p>
                    </div>
                </div>
                <%}%>
            </form>
        </div>
    </section>


    <script type="text/javascript">
        const menu = document.querySelector(".menu");
        const tog = document.querySelector(".tog");
        menu.addEventListener("click", () => {
            tog.classList.toggle("active");
        });

        const close = document.querySelector(".close");
        close.addEventListener("click", () => {
            tog.classList.remove("active");
        });
        document.addEventListener("click", (event) => {
            if (!tog.contains(event.target) && !menu.contains(event.target)) {
                tog.classList.remove("active");
            }
        });
        var modal = document.getElementById("myModal");
        if (modal) {
            modal.style.display = "block";
        }
        function closeModal() {
            modal.style.display = "none";

        }
        window.onclick = function(event) {
            if (event.target === modal) {
                modal.style.display = "none";
            }
        }
    </script>

